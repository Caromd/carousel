

/** ---------------------------------------------------------------------------------------------------- **/

.result_monthly, .result_monthly span
{
	color: inherit !important;
}
.result_monthly strong
{
	font-size: inherit !important;
	line-height: inherit !important;
	letter-spacing: inherit !important;
	text-indent: inherit !important;
}

/** ---------------------------------------------------------------------------------------------------- **/

#content p.large-line-height
{
	line-height: 32px;
}

/** ---------------------------------------------------------------------------------------------------- **/

#progres_bar
{
	background: #efefef;
	width: 80%;
	height: 30px;
	margin: 0px auto;
}
#progress
{
	background: #014286;
	height: 30px;
	position: relative;
}
#progress:after
{
	content: "";
    position: absolute;
    top: 0;
    right: 0;
    border-width: 0px 0px 30px 30px;
    border-style: solid;
    border-color: transparent transparent #efefef #014286;
}

.icon_customers_dash
{
	height: 140px;
}
.block_customers_dash_green h5,
.block_customers_dash_green h5 a
{
	color: #82cd77 !important;
}
.block_customers_dash_grey h5,
.block_customers_dash_grey h5 a,
.block_customers_dash_grey a
{
	color: #ccc !important;
	cursor: default !important;
}
.block_customers_dash_grey a:hover
{
	text-decoration: none;
}


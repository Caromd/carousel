/** ---------------------------------------------------------------------------------------------------- **/
/**
 *  Tables
 */
table
{
    width: 100%
}
table table
{
    background-color: #ffffff
}
table tr td,
table tr th,
table tr td a,
table tr th a
{
	font-size: 12px;
}
table > thead > tr > td,
table > thead > tr > th
{
    cursor: inherit;
    padding: 5px 10px;
    text-transform: uppercase;
    color: inherit;
    background: #f5f5f5;
    background: -moz-linear-gradient(top,  #f5f5f5 0%, #dedede 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f5f5f5), color-stop(100%,#dedede));
    background: -webkit-linear-gradient(top,  #f5f5f5 0%,#dedede 100%);
    background: -o-linear-gradient(top,  #f5f5f5 0%,#dedede 100%);
    background: -ms-linear-gradient(top,  #f5f5f5 0%,#dedede 100%);
    background: linear-gradient(to bottom,  #f5f5f5 0%,#dedede 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f5f5f5', endColorstr='#dedede',GradientType=0 );
}
table thead tr td.sorted,
table thead tr td.no-sort.sorted:hover
{
    cursor: pointer;
    background: #0064a7;
    border-color: #0064a7;
    color: #fff;
}
table thead tr td
{
    font-weight: 600
}
table tbody tr td
{
    padding: 5px 10px;
}
table > tbody > tr:nth-child(even) > td,
table table > tbody > tr:hover:nth-child(even) > td
{
    background: #efefef
}
table  table > tbody > tr:hover:nth-child(odd) > td
{
    background: #FFFFFF
}
table.noaltrows > tbody > tr td
{
    background: #fff
}
table > tbody > tr:hover > td
{
    background: #e5ecf2
}
table tfoot tr td
{
    padding: 15px;
    border-top: 2px solid #eee;
}
table tfoot tr td ol,
table tfoot tr td ul
{
    margin: 0;
    padding: 0;
    list-style: none;
}
table.breakdownTable tr
{
    background: #f7f7f7
}
table.breakdownTable tr.childrow,
table.breakdownTable tr.childrow td
{
    background: white
}
table.breakdownTable tr.childrow td
{
    border-bottom: 1px solid #efefef
}
table.no-style,
table.no-style tr,
table.no-style tr:hover td,
table.no-style tr td
{
    margin: 0;
    padding: 0;
    background: none;
}
table.no-bg,
table.no-bg tr,
table.no-bg tr:hover,
table.no-bg td,
table.no-bg td:hover,
table tr.no-bg td,
table tr.no-bg
{
	background: none !important;
}
table.double-header
{
    border-spacing: 0;
    border-collapse: 0;
}
table.double-header thead tr td
{
    border-right: 1px solid #e9e9e9
}
table.double-header thead tr:first-child td
{
    margin-bottom: 0;
    border-bottom: 1px solid #e9e9e9;
}
table.double-header thead tr:last-child td
{
    margin-top: 0;
    text-align: center;
}
table.double-header thead tr td:hover
{
    background-color: #f5f5f5;
    border-bottom-color: #eee;
    color: inherit;
}
table tbody tr.green td
{
    background: #eeffef
}
table tbody tr.green:nth-child(even) td
{
    background: #d2f7d4
}
table tbody tr.red td
{
    background: #fff1f1
}
table tbody td.green
{
    background: #d2f7d4 !important
}
table tbody td.red
{
    background: #f9c7c7 !important
}
table tbody td.blue
{
	background: #e5ecf2 !important;
}
table tbody td.blue2
{
	background: #cce0f1 !important;
}
table tbody td.colmatch
{
    background: #c1f1f5 !important
}
table tbody td.yellow,
table tbody tr.yellow th,
table tbody tr.yellow td
{
    background: #FCE5B4 !important
}
table.trborders tr td,
table.trborders tr th
{
	border-bottom: 1px solid #efefef;
}
table.collapse
{
	border-collapse: collapse;
}
table td.nopad_left
{
	padding-left: 0px;
}

/** ---------------------------------------------------------------------------------------------------- **/
/**
 *  Messages
 */

div.message
{
  width: 100%;
  margin: 10px 0;
  padding: 5px;
  background: #eee;
  border: 1px solid #aaa;
  border-radius: 2px;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
}
div.message.payoneer-status {
    margin: 0;
    display: inline-block;
    width: 70%;
}
div.message-progress
{
  width: 100%;
  margin: 10px auto;
  padding: 5px 5px 0;
  background: #eee;
  border: 1px solid #aaa;
  float: none;
  display: block;
  border-radius: 2px;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  -moz-box-sizing: border-box; 
  -webkit-box-sizing: border-box; 
  box-sizing: border-box; 
  background: #fffae7;

}
div.message-progress ul
{
    list-style-image: url("../img/alert_icon_warning.png");
}

div.message:hover
{
  cursor: pointer;
}

div.message.success
{
  background: url(../img/alert_icon_success.png) 10px 9px no-repeat #eeffef;
  border-color: #80d987;
}

div.message.error
{
  background: url(../img/alert_icon_error.png) 10px 9px no-repeat #fff1f1;
  border-color: #ffbebe;
}

#error-message-box {
  padding: 25px 0 0 20px;
}

#error-message-box.message.error label {
  margin-left: 10px;
}

div.message.warning
{
  background: url(../img/alert_icon_warning.png) 10px 9px no-repeat #fffae7;
  border-color: #ffe787;
}

div.message.info
{
  background: url(../img/alert_icon_info.png) 10px 9px no-repeat #ebfaff;
  border-color: #a7e7ff;
}

div.message.flash,
div.message.persistent
{
  width: 950px;
  margin: 10px auto 0 auto;
  display: block;
}

div.message > span
{
  margin-left: 30px;
  display:block;
}

div.message a
{
  text-decoration: underline;
}

div.message.success > span,
div.message.success a
{
  color: green;
}

div.message.error > span,
div.mesasge.error a,
div.error a
{
  color: #d30000;
}

div.message.warning > span,
div.message.warning a
{
  color: #cc8026;
}

div.message.info > span,
div.message.info a
{
  color: #0064a7;
}

span.error
{
  font-weight: 600;
  color: #d30000;
}

span.error:before
{
  content: '^';
  width: 16px;
  height: 16px;
  margin-right: 2px;
  display: inline-block;
  background: url(../img/icons/red/16x16/ic_up.png) center no-repeat;
  text-indent: -10000%;
}





.debug
{
	color: red;
}


ul.ui-autocomplete
{
	max-height: 240px;
	overflow-y: scroll;
	overflow-x: hidden;
}

.icon16x16_inline
{
	margin: 0px 4px -2px 0px;
}




.th_order_by
{
	text-indent: -9999px;
	height: 0;
	width: 0;
	border: 8px solid transparent;
	display: block;
	float: right;
}
.th_order_by:hover
{
	opacity: 0.5;
}
/* arrow up */
.th_order_by.th_order_by_asc
{
	border-bottom-color: #000;
	border-top: 0px;
	margin-top: 9px;
}
/* arrow down */
.th_order_by.th_order_by_desc
{
	border-top-color: #000;
	border-bottom: 0px;
	margin-top: 9px;
}

#content h3.boxaround
{
	border: 3px solid #000;
	display: inline;
	padding: 8px 16px;
	font-weight: bold;
}
#content h3.boxaround.success
{
	border-color: #51a542;
	color: #51a542;
}
#content h3.boxaround.error
{
	border-color: #d30000;
	color: #d30000;
}
#content h3.boxaround.pending
{
	border-color: #ff9900;
	color: #ff9900;
}
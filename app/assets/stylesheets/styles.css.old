body
{
	background: #efefef;
}
.w
{
	width: 940px;
	margin: 0px auto;
}
h1
{
	font-size: 28px;
	line-height: 30px;
}
#content
{
	background: white;
	padding: 30px;
}
#content p,
#content ul li,
#content p a,
#content table tr td,
#content ol li
{
	font-size: 14px;
	line-height: 28px;
}
label,
input,
select,
fieldset span
{
	font-size: 13px;
	line-height: 26px;
}
#content p a
{
	text-decoration: underline;
}
#content h2
{
	color: #014283;
	font-weight: 300;
	border: 0px;
	font-size: 28px;
	line-height: 32px;
}
h3,
#content h3,
#content h3 span,
#content ul.large,
#content table td.large
{
	color: #014283;
	font-weight: 400;
	border: 0px;
	font-size: 20px;
	line-height: 32px;
}
#content h4,
#content h4 span
{
	color: #014283;
	font-weight: 300;
	border: 0px;
	font-size: 16px;
	line-height: 32px;
}
#content h5,
#content h5 a
{
	font-weight: bold;
	font-size: 16px;
	color: #014286;
}
#content ul.large,
#content ul.large li
{
	font-size: 16px;
}
#content ul.large span,
#content ul li span
{
	color: #999;
	margin-left: 10px;
}
#content ul li
{
	padding-top: 2px;
	padding-bottom: 2px;
}
#content ul li span
{
	font-size: 11px;
	margin-left: 5px;
}

/** ---------------------------------------------------------------------------------------------------- **/

#header
{

}
#header h1 a
{
	background: url('logo.png');
	width: 239px;
	height: 104px;
	display: block;
	text-indent: -9999px;
	float: left;
	margin: 15px 0px 15px 0px;
}
#header #nav
{
	float: right;
	text-align: right;
	padding: 60px 0px 6px 0px;
	margin: 0px;
}
#header #nav a
{
	text-transform: uppercase;
	font-size: 16px;
	font-weight: 600;
	color: #014283;
	padding: 4px 8px;

	-webkit-border-radius: 6px;
	-moz-border-radius: 6px;
	border-radius: 6px;
}
#header #nav a.current
{
	background: #014283;
	color: white;
}
#header #loggedin
{
	float: right;
	font-size: 13px;
	color: #999;
}
#header #loggedin strong
{
	color: black;
}
#blueline
{
	background: #014283;
	height: 6px;
	margin: 0px 0px 0px 0px;
}

/** ---------------------------------------------------------------------------------------------------- **/

#banner
{
	height: 120px;
}
.banner_bluefade
{
	height: 100%;
	text-align: center;
	color: white;

	background: #1262ad;
	background: -moz-linear-gradient(top,  #1262ad 0%, #013d7b 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#1262ad), color-stop(100%,#013d7b));
	background: -webkit-linear-gradient(top,  #1262ad 0%,#013d7b 100%);
	background: -o-linear-gradient(top,  #1262ad 0%,#013d7b 100%);
	background: -ms-linear-gradient(top,  #1262ad 0%,#013d7b 100%);
	background: linear-gradient(to bottom,  #1262ad 0%,#013d7b 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1262ad', endColorstr='#013d7b',GradientType=0 );
}
.banner_bluefade h1
{
	line-height: 110px;
	font-weight: 300;
	font-size: 36px;
}


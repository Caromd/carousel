}
body .qtipForm .qtip-close:hover
{
	
}

/** ---------------------------------------------------------------------------------------------------- **/

#calendar tbody td
{
	text-align: center;
	position: relative;
    vertical-align: top;
}
#calendar tbody td.SCprefix time,
#calendar tbody td.SCsuffix time,
#calendar tbody td.SCdisable time
{
	color: #ccc;
}
#calendar tbody td time
{
	color: #666;
	padding: 6px;
	display: block;
}
#calendar tbody td.today
{
	background: #ccd9e6;
}
#calendar tbody td.SCnormFuture:not(.SCsun):hover,
#calendar tbody td.today:not(.SCsun):hover,
#calendar tbody td.SCselectable:hover
{
	background: #80a0c1;
	cursor: hand;
	cursor: pointer;
}
#calendar tbody td.SCnormFuture:not(.SCsun):hover time,
#calendar tbody td.today:not(.SCsun):hover time,
#calendar tbody td.selected time,
#calendar tbody td.SCselectable:hover time
{
	color: white;
}
#calendar tbody td.selected,
#calendar tbody td.selected:hover
{
	background: #014283;
}
#calendar_nav_title
{
	position: absolute;
	width: 390px;
}
#nextMonth,
#prevMonth
{
	position: relative;
	z-index: 999;
}

/** ---------------------------------------------------------------------------------------------------- **/

#blue_nav
{
	background: #014283;
	margin-top: -6px;
}
#blue_nav ul
{
	margin: 0px;
	padding: 0px;
	list-style-type: none;
}
#blue_nav li
{
	float: left;
	position: relative;
}
#blue_nav a
{
	display: block;
	color: white;
	padding: 10px 14px 10px 14px;
	font-weight: bold;
	text-transform: uppercase;
}
#blue_nav a.disabled_link
{
	opacity: 0.5;
	cursor: default !important;
}
#blue_nav a.disabled_link:hover
{
	background: inherit !important;
}
#blue_nav a:hover
{
	text-decoration: none;
}
#blue_nav a.current,
#blue_nav a:hover
{
	background: #34689c;
}

#blue_nav ul li ul
{
    width: auto;
    height: auto;
    left: 0;
    position: absolute;
    display: none;
    list-style-type: none;
    width: 200px;
    z-index: 999;
}
#blue_nav > ul > li:hover ul
{
    display: block;
}
#blue_nav ul li ul li
{
	width: 100%;
	display: block;
	clear: both;
	background-color: #014283;
	border-top: 1px solid #34689c;
	list-style-type: none;
}

.or_vertical_line
{
	border-right: 1px solid #ccc;
	position: relative;
}
.or_vertical_line span
{
	font-weight: bold;
	position: absolute;
	background: white;
	font-size: 14px;
	right: -21px;
	top: 20px;
}



/** ---------------------------------------------------------------------------------------------------- **/

/**
 *  Segmented Control
 */

ol.seg-control,
ol.seg-control-opts,
ol.seg-control-links,
ol.control-options
{
  margin: 0;
  padding: 0;
  display: inline-block;
}

ol.seg-control li,
ol.seg-control-links li
{
        margin: 0;
        padding: 7px 10px 4px 10px;
        display: inline-block;
        float: left;
        background: url(../img/button_bg.png) 0 0 repeat-x #f3f3f3;
        border-top: 1px solid #c9cacc;
        border-bottom: 1px solid #c9cacc;
        border-right: 1px solid #ddd;
        box-shadow: 1px 1px 0 #fff;
}

ol.seg-control li a,
ol.seg-control-links li a
{
    font-size: 15px;
}

ol.seg-control li:first-child,
ol.seg-control-links li:first-child
{
        border-left: 1px solid #c9cacc;
        border-top-left-radius: 2px;
        border-bottom-left-radius: 2px;
        -moz-border-radius-topleft: 2px;
        -moz-border-radius-bottomleft: 2px;
        -webkit-border-top-left-radius: 2px;
        -webkit-border-bottom-left-radius: 2px;
}

ol.seg-control li:last-child,
ol.seg-control-links li:last-child
{
        border-right: 1px solid #c9cacc;
        border-top-right-radius: 2px;
        border-bottom-right-radius: 2px;
        -moz-border-radius-topright: 2px;
        -moz-border-radius-bottomright: 2px;
        -webkit-border-top-right-radius: 2px;
        -webkit-border-bottom-right-radius: 2px;
}

ol.seg-control li:hover,
ol.seg-control-links li:hover
{
	background: #3ec5d7;
	border-color: #209fab;
	cursor: pointer;
}

ol.seg-control li:hover a,
ol.seg-control li.current a,
ol.seg-control-links li:hover a,
ol.seg-control-links li.current a
{
	color: #fff;
	text-decoration: none;
}

ol.seg-control li.current,
ol.seg-control-links li.current
{
	background: #2f5f96;
	border-color: #2f5f96;
}

ol.seg-control li.current:hover,
ol.seg-control-links li.current:hover
{
	background: #2a5587;
}

ol.seg-control.dashboard-fixed-width,
ol.seg-control.dashboard-fixed-width-four
{
  width: 468px;
}

ol.seg-control.dashboard-fixed-width li
{
  width: 134px;
}

ol.seg-control.dashboard-fixed-width-four li {
	width: 95px;
}

ol.seg-control.live-marketplace-fixed-width
